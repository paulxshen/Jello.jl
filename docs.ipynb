   "source": [
    "using Random, Jello, CairoMakie\n",
    "\n",
    "Random.seed!(1)\n",
    "l = 100\n",
    "lvoid = 6\n",
    "lsolid = 6\n",
    "init = nothing\n",
    "m = Blob(l, l; init, lvoid, lsolid)\n",
    "# m = gpu(m)\n",
    "sharpness = 0.99\n",
    "a = m(sharpness)\n",
    "\n",
    "fig = Figure()\n",
    "grid = fig[1, 1]\n",
    "ax, plt = heatmap(grid[1, 1], m())\n",
    "Colorbar(grid[1, 2], plt)\n",
    "display(fig)"
   ]
